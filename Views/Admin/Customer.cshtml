<div class="buy-now">
    <a class="btn btn-primary btn-buy-now text-white" data-bs-toggle="modal"
       data-bs-target="#largeModal">Send Promo</a>
</div>

<div class="modal fade" id="largeModal" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel3">Send Promo</h5>
                <button type="button"
                        class="btn-close"
                        data-bs-dismiss="modal"
                        aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col mb-3">
                        <label for="nameLarge" class="form-label">Message</label>
                        <textarea type="text" id="nameLarge" class="form-control" placeholder="Enter Promo Message" rows="4"> </textarea>
                    </div>
                </div>
               
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-secondary" data-bs-dismiss="modal">
                    Close
                </button>
                <button type="button" class="btn btn-primary">Send Promo!</button>
            </div>
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        $("#GetallData").click(function () {
            $("#tblPopulation").show();
            $.ajax({
                url: 'https://api.publicapis.org/entries',
                dataType: "json",
                type: "Get",
                //contentType: 'application/json; charset=utf-8',
                //data: {Id:Id},
                success: function (data) {
                    var tr;
                    console.log(data.entries);
                    for (var i = 0; i < data.entries.length; i++) {
                        tr = tr + "<td>" + data[i].API + "</td>";
                        tr = tr + "<td>" + data[i].Description + "</td>";
                        tr = tr + "<td>" + data[i].Link + "</td>";
                        tr = tr + "<td>" + data[i].Cors + "</td>";
                    }
                    $('#test1').append(tr);
                    tblFormate();
                },
                error: function (xhr) {
                    alert('No Valid Data');
                }
            });
        });
    })
</script>
